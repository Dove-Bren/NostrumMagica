{
    "multipart": [
		{
			"apply": { "model": "nostrummagica:block/duct_center" }
		},
		{
			"when": { "north": "true" },
			"apply": { "model": "nostrummagica:block/duct_side" }
		},
		{
			"when": { "east": "true" },
			"apply": { "model": "nostrummagica:block/duct_side", "y": 90 }
		},
		{
			"when": { "south": "true" },
			"apply": { "model": "nostrummagica:block/duct_side", "y": 180 }
		},
		{
			"when": { "west": "true" },
			"apply": { "model": "nostrummagica:block/duct_side", "y": 270 }
		},
		{
			"when": { "up": "true" },
			"apply": { "model": "nostrummagica:block/duct_side", "x": 270 }
		},
		{
			"when": { "down": "true" },
			"apply": { "model": "nostrummagica:block/duct_side", "x": 90 }
		}
	]
}